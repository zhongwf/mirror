{
  "manifest_version": 2,

      "name": "反射镜",
    "version": "1.0.1",
    "manifest_version": 2,
    "description": "用360镜像替换google前端库和字体库，以此访问部分外国网站。",
	"update_url":"https://raw.githubusercontent.com/zhongwf/mirror/master/updates.xml",

    "icons": {
        "128": "images/logo.png"
    },

		"background": {
        "scripts": ["js/background.js"]
    },
  
  "browser_action": {
    "default_icon": "images/logo.png",
	"default_title": "反射镜",
	"default_popup": "popup.html"
  },  
  
  "permissions": [
    "activeTab",
		"tabs",
        "notifications",
        "cookies",
        "webRequest",
        "webRequestBlocking",
        "http://*/*",
		"https://*/*",
		"storage"
  ]
  

}
